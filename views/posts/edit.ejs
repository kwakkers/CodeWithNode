<h1>Edit Post!</h1>
<form action="/posts/<%= post.id %>?_method=PUT" method="POST">
    <div>
        <input type="text" name="title" value="<%= post.title %>">
    </div>
    <div>
        <input type="text" name="price" value="<%= post.price %>">
    </div>
    <div>
        <textarea name="description"><%= post.description %></textarea>
    </div>
    <div>
        <input type="text" name="location" value="<%= post.location %>">
    </div>
    <input type="submit">
    
    
</form>